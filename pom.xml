  <modelVersion>4.0.0</modelVersion>

  <groupId>de.mobilcom</groupId>
  <artifactId>edge-cli</artifactId>
  <version>v0.1.0</version>
  <packaging>jar</packaging>

  <name>edge-cli</name>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
